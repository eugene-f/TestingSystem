<DATABASE>
    <Platform
            tutors="ListOfTutor"
            students="ListOfStudent"
            disciplines="ListOfDiscipline"
            tests="ListOfTest"/>

    <abstractBaseEntity
            uuid="UUID"
            id="Long"
            deleted="boolean">

        <abstractBaseUser
                firstName="String"
                lastName="String"
                email="String"
                passwordHash="String">
            <Tutor
                    createdTests="ListOfTest"
                    completedSessions="ListOfSession"/>
            <Student
                    completedSessions="ListOfSession"
                    currentSession="Session"
                    toPassTests="ListOfTest"/>
        </abstractBaseUser>



        <Test
                tutor="Tutor"
                discipline2="Discipline"
                author="Tutor"
                name="String"
                discipline="Discipline"
                questions="ListOfQuestion" minutesToPass="byte"
                publicated="boolean"
                completedSessions="ListOfSession"/>
        <Session
                student2="Student"
                test2="Test"
                test="Test"
                student="Student"
                startDate="Date"
                completedQuestionAnswerMap="MapOfQuestionAndAnswer"
                result="byte"/>
        <Question
                test="Test"
                content="String"
                answers="ListOfAnswer"
                trueAnswer="Answer"/>
        <Discipline
                name="String"
                tests="ListOfTest"/>
        <Answer
                question="Question"
                content="String"
                correct="boolean"/>

    </abstractBaseEntity>

</DATABASE>